  {
    "type": "paragraph_text",
    "value": " from all his troubles."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " "
  },
  {
    "type": "divine_name",
    "value": "YHWH"
  },
  {
