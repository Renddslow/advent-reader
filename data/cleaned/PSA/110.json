[
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " Here is "
  },
  {
    "type": "divine_name",
    "value": "YHWH"
  },
  {
    "type": "paragraph_text",
    "value": "’s proclamation"
  },
  {
    "type": "paragraph_text",
    "value": " to my lord:"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "“Sit down at my right hand"
  },
  {
    "type": "paragraph_text",
    "value": " until I make your enemies your footstool!”"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " "
  },
  {
    "type": "divine_name",
    "value": "YHWH"
  },
  {
    "type": "paragraph_text",
    "value": " extends"
  },
  {
    "type": "paragraph_text",
    "value": " your dominion"
  },
  {
    "type": "paragraph_text",
    "value": " from Zion."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "Rule in the midst of your enemies!"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " Your people willingly follow you"
  },
  {
    "type": "paragraph_text",
    "value": " when you go into battle."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "On the holy hills"
  },
  {
    "type": "paragraph_text",
    "value": " at sunrise"
  },
  {
    "type": "paragraph_text",
    "value": " the dew of your youth"
  },
  {
    "type": "paragraph_text",
    "value": " belongs to you."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " "
  },
  {
    "type": "divine_name",
    "value": "YHWH"
  },
  {
    "type": "paragraph_text",
    "value": " makes this promise on oath"
  },
  {
    "type": "paragraph_text",
    "value": " and will not revoke it:"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "“You are an eternal priest"
  },
  {
    "type": "paragraph_text",
    "value": " after the pattern of"
  },
  {
    "type": "paragraph_text",
    "value": " Melchizedek.”"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " O sovereign "
  },
  {
    "type": "divine_name",
    "value": "YHWH"
  },
  {
    "type": "paragraph_text",
    "value": ","
  },
  {
    "type": "paragraph_text",
    "value": " at your right hand"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "he strikes down"
  },
  {
    "type": "paragraph_text",
    "value": " kings in the day he unleashes his anger."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " He executes judgment"
  },
  {
    "type": "paragraph_text",
    "value": " against"
  },
  {
    "type": "paragraph_text",
    "value": " the nations;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "he fills the valleys with corpses;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "he shatters their heads over the vast battlefield."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " From the stream along the road he drinks;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "then he lifts up his head."
  },
  {
    "type": "paragraph_end"
  }
]