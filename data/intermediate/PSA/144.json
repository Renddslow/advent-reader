[
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " The "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": ", my protector,"
  },
  {
