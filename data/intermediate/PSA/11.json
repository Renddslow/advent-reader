[
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " In the "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " I have taken shelter."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "How can you say to me,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "“Flee to a mountain like a bird!"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " For look, the wicked"
  },
  {
    "type": "paragraph_text",
    "value": " prepare"
  },
  {
    "type": "paragraph_text",
    "value": " their bows,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "they put their arrows on the strings,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "to shoot in the darkness"
  },
  {
    "type": "paragraph_text",
    "value": " at the morally upright."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " When the foundations"
  },
  {
    "type": "paragraph_text",
    "value": " are destroyed,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "what can the godly"
  },
  {
    "type": "paragraph_text",
    "value": " accomplish?”"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " The "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " is in his holy temple;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "the "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": "’s throne is in heaven."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "His eyes"
  },
  {
    "type": "paragraph_text",
    "value": " watch;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "his eyes"
  },
  {
    "type": "paragraph_text",
    "value": " examine"
  },
  {
    "type": "paragraph_text",
    "value": " all people."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " The "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " approves of"
  },
  {
    "type": "paragraph_text",
    "value": " the godly,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "but he"
  },
  {
    "type": "paragraph_text",
    "value": " hates"
  },
  {
    "type": "paragraph_text",
    "value": " the wicked and those who love to do violence."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " May the "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " rain down"
  },
  {
    "type": "paragraph_text",
    "value": " burning coals"
  },
  {
    "type": "paragraph_text",
    "value": " and brimstone"
  },
  {
    "type": "paragraph_text",
    "value": " on the wicked!"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "A whirlwind is what they deserve!"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " Certainly"
  },
  {
    "type": "paragraph_text",
    "value": " the "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " is just;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "he rewards godly deeds;"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "the upright will experience his favor."
  },
  {
    "type": "paragraph_end"
  }
]