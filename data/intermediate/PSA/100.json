  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": " For the "
  },
  {
    "type": "divine_name",
    "value": "Lord"
  },
  {
    "type": "paragraph_text",
    "value": " is good."
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "His loyal love endures,"
  },
  {
    "type": "paragraph_end"
  },
  {
    "type": "paragraph_start"
  },
  {
    "type": "paragraph_text",
    "value": "and he is faithful through all generations."
  },
  {
    "type": "paragraph_end"
  }
